{
  "version": 2,
  "builds": [
    {
      "src": "home.html",
      "use": "@vercel/static"
    },
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/$",
      "dest": "/home.html"
    },
    {
      "src": "/screen_saver",
      "dest": "/screen_saver.html"
    },
    {
      "src": "/error_404",
      "dest": "/ERROR_404.html",
      "status": 404
    }
  ]
}
