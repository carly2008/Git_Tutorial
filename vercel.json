{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
    "rewrites": [
      { "source": "/JS-DOS/(.*)", "destination": "/JS-DOS/$1" },
      { "source": "/GAMES/(.*)", "destination": "/GAMES/$1" }
    ]  
}
